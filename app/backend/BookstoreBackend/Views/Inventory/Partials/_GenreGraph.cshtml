@{


    var orders_genre = (Dictionary<string, int>)ViewData["orders_genre"];



}
<canvas id="genreChart" width="250" height="250"></canvas>


<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.bundle.min.js"></script>
<script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>


<script type="text/javascript">

        $(function () {
    var chartName = "genreChart";
            var ctx = document.getElementById(chartName).getContext('2d');

        var data = {
            labels: ["@orders_genre.Keys.ElementAt(0)"],@*,"@orders_genre.Keys.ElementAt(1)","@orders_genre.Keys.ElementAt(2)","@orders_genre.Keys.ElementAt(3)","@orders_genre.Keys.ElementAt(4)"*@
                datasets: [{
                    label: "Top 5 Genres",
                    backgroundColor: [
                        'rgba(255, 99, 132, 0.8)',
                        'rgba(54, 162, 235, 0.8)',
                        'rgba(255, 206, 86, 0.8)',
                        'rgba(75, 192, 192, 0.8)',
                        'rgba(153, 102, 255, 0.8)',
                        'rgba(255, 159, 64, 0.8)',
                        'rgba(255, 0, 0)',
                        'rgba(0, 255, 0)',
                        'rgba(0, 0, 255)',
                        'rgba(192, 192, 192)',
                        'rgba(255, 255, 0)',
                        'rgba(255, 0, 255)'
                    ],
                    borderColor: [
                        'rgba(255,99,132,1)',
                        'rgba(54, 162, 235, 1)',
                        'rgba(255, 206, 86, 1)',
                        'rgba(75, 192, 192, 1)',
                        'rgba(153, 102, 255, 1)',
                        'rgba(255, 159, 64, 1)',
                        'rgba(255, 0, 0)',
                        'rgba(0, 255, 0)',
                        'rgba(0, 0, 255)',
                        'rgba(192, 192, 192)',
                        'rgba(255, 255, 0)',
                        'rgba(255, 0, 255)'
                    ],
                    borderWidth: 1,
                    data: [@orders_genre.Values.ElementAt(0)] @*@orders_genre.Values.ElementAt(1),@orders_genre.Values.ElementAt(2),@orders_genre.Values.ElementAt(3),@orders_genre.Values.ElementAt(4)*@
    }]
            };

            var options = {
                maintainAspectRatio: true,

            };

       var myChart = new  Chart(ctx, {
                options: options,
                data: data,
                type:'pie'

            });
        });
</script>
