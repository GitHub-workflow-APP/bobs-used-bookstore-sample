@{
    var orders_genre = (Dictionary<string, int>)ViewData["orders_genre"];
    var orders_type = (Dictionary<string, int>)ViewData["orders_type"];
    var orders_publisher = (Dictionary<string, int>)ViewData["orders_publisher"];
    var orders_name = (Dictionary<string, int>)ViewData["orders_name"];

    var inventory_stats = (List<int>)ViewData["Inventory"];

    var total_titles = inventory_stats[0];
    var total_books = inventory_stats[1];
    var total_merchandise_value = inventory_stats[2];
    var genre_count = inventory_stats[3];
    var publisher_count = inventory_stats[4];
    var type_count = inventory_stats[5];
    var conditions_count = inventory_stats[6];

    var order_stats = (List<int>)ViewData["Orders"];
    var ordersplaced = order_stats[0];
    var delivered = order_stats[1];
    var enroute = order_stats[2];
    var pending = order_stats[3];
    var justplaced = order_stats[4];
    var total_books_sold = order_stats[4];
    var total_sales = order_stats[6];
}

<head>
    <link rel="stylesheet" href="/css/Inventory/dashboard.css"/>
</head>

<div class="" style="margin-left:15%;margin-right:15%">
    <div class="row" style="margin-bottom: 30px;">
        <h3 class="col-12 website-text text-center">@Resource.InventoryDashboardLabel</h3>
    </div>
    <div class="row d-flex justify-content-center">
        <partial name="./Partials/_BookCardsStats" view-data="ViewData"/>
    </div>

    <div class="row" style="margin-top:35px;">
        <partial name="./Partials/_OrderStatsGraph" view-data="ViewData"/>
    </div>

    <div class="row" style="margin-top:35px;">
        <div class="col-lg-4 col-md-4 col-sm-3">
            <h5 class="website-text text-center">@Resource.TopGenresLabel</h5>
            <partial name="./Partials/_GenreGraph" view-data="ViewData"/>
        </div>

        <div class="col-lg-4 col-md-4 col-sm-3">
            <h5 class="website-text text-center">@Resource.TopTypesLabel</h5>
            <partial name="./Partials/_TypeGraph" view-data="ViewData"/>
        </div>

        <div class="col-lg-4 col-md-4 col-sm-3">
            <h5 class="website-text text-center">@Resource.TopPublishersLabel</h5>
            <partial name="./Partials/_PublisherGraph" view-data="ViewData"/>
        </div>

        <div class="col-lg-4 col-md-4 col-sm-3" style="margin-top:25px;">
            <h5 class="website-text text-center">@Resource.TopTitlesLabel</h5>
            <partial name="./Partials/_BookNameGraph" view-data="ViewData"/>
        </div>
    </div>
</div>