@model BOBS_Backend.Models.AdminUser
@{
    ViewData["Title"] = "Register";
}
<p>@ViewData["Register"]</p>

<form class="form-horizontal" asp-action="RegisterPage">
    <div class="form-group">
        <label class="control-label col-sm-2" for="email">UserName</label>
        <div class="col-sm-10">
            <input type="UserName" class="form-control" asp-for="Username" placeholder="Enter UserName">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="pwd">Password:</label>
        <div class="col-sm-10">
            <input type="password" class="form-control" id="pwd" asp-for="password" placeholder="Enter password">
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-sm-2" for="pwd">email</label>
        <div class="col-sm-10">
            <input type="email" class="form-control" id="email" asp-for="email" placeholder="email">
        </div>
    </div>

    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-default btn-block">Submit</button>
        </div>
    </div>
</form>